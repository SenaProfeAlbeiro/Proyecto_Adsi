Algoritmo taller1_Ejercio1
	
	// Declarar Variables
	Definir num1 Como Real;
	Definir num2 Como Real;
	Definir res Como Real;
	
	// Definir Variables
	num1<-0.0;
	num2<-0.0;
	res<-0.0;
	
	// Entrada Datos
	Escribir "Digite el primer número: ";
	Leer num1;
	Escribir "Digite el segundo número: ";
	Leer num2;
	
	// Proceso 
	res <- num1 + num2; 
	
	// Salida Datos
	Escribir "La Suma de ", num1, " mas ", num2, " es = ", res;
	
FinAlgoritmo
